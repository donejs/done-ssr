    "through2": "^2.0.1",
    "uglify-js": "3.2.0"
  },
  "dependencies": {
    "can-namespace": "^1.0.0",
    "can-reflect": "^1.11.0",
    "can-util": "^3.10.10",
    "can-vdom": "^3.2.5",
    "can-zone": "^0.6.11",
